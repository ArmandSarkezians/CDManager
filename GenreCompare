// This class compares two songs by their genres (alphabetically)

package AssignmentFour;

import java.util.*;
public class GenreCompare implements Comparator <Song>{

    public int compare (Song song1, Song song2){
        return song1.getGenre().compareTo (song2.getGenre());
    }
}
